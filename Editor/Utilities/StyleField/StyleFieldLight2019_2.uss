.unity-style-field__options-popup > .unity-base-field__input {
    background-image: resource("Builtin Skins/LightSkin/Images/btn mid.png");
}

.unity-style-field__options-popup > .unity-base-field__input:hover:active {
    background-image: resource("Builtin Skins/LightSkin/Images/btn mid act.png");
}

.unity-style-field__options-popup > .unity-base-field__input:checked {
    background-image: resource("Builtin Skins/LightSkin/Images/btn mid on.png");
}

.unity-style-field__options-popup > .unity-base-field__input:hover:active:checked {
    background-image: resource("Builtin Skins/LightSkin/Images/btn mid onact.png");
}

.unity-style-field__options-popup > .unity-base-field__input:checked:focus {
    background-image: resource("Builtin Skins/LightSkin/Images/btn mid on focus.png");
}