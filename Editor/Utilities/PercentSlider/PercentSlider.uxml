<UXML xmlns="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements">
    <VisualElement name="unity-visual-input" class="unity-percent-slider__visual-input">
        <SliderInt
            name="unity-slider"
            class="unity-percent-slider__slider"
            step-size="1"
            low-value="0"
            high-value="100" />
        <uie:IntegerField
            name="unity-field"
            class="unity-percent-slider__field"
            max-length="3" />
    </VisualElement>
</UXML>