.unity-button-strip {
    flex-direction: row;
}

.unity-button-strip__button {
    padding-bottom: 3px;
    -unity-font-style: normal;
}

.unity-button-strip__button--left {
    -unity-slice-right: 1;
    padding-left: 6px;
}

.unity-button-strip__button--mid {
    -unity-slice-left: 1;
    -unity-slice-right: 1;
}

.unity-button-strip__button--right {
    -unity-slice-left: 1;
    padding-right: 6px;
}

.unity-button-strip__button-icon {
    width: 16px;
    height: 16px;
}