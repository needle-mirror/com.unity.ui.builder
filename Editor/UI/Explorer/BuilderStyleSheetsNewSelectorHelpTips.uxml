<UXML xmlns="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements">
    <VisualElement class="unity-builder-code-row">
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-type" text="ElementCSharpType" />
        <Label class="unity-builder-code-label--code" text=" (no symbols)" />
    </VisualElement>
    <VisualElement class="unity-builder-code-row">
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-name" text="#elementNameOrId" />
        <Label class="unity-builder-code-label--code" text=" (start with #)" />
    </VisualElement>
    <VisualElement class="unity-builder-code-row">
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-class-name" text=".styleClassName" />
        <Label class="unity-builder-code-label--code" text=" (start with .)" />
    </VisualElement>
    <VisualElement class="unity-builder-code-row">
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-class-name" text=".parentClassName" />
        <Label class="unity-builder-code-label--code" text=" > " />
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-class-name" text=".directChildClassName" />
    </VisualElement>
    <VisualElement class="unity-builder-code-row">
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-class-name" text=".parentClassName" />
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-class-name" text=" .childClassName" />
        <Label class="unity-builder-code-label--code" text=" (at any depth)" />
    </VisualElement>
    <VisualElement class="unity-builder-code-row">
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-class-name" text=".styleClassName" />
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-pseudo-state" text=":hover" />
        <Label class="unity-builder-code-label--code" text=" (only on mouse over)" />
    </VisualElement>
    <VisualElement class="unity-builder-code-row">
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-class-name" text=".styleClassName" />
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-pseudo-state" text=":focus" />
        <Label class="unity-builder-code-label--code" text=" (only when in focus)" />
    </VisualElement>
    <VisualElement class="unity-builder-code-row unity-builder-code-row--padded">
        <Label class="unity-builder-code-label" text="Example:" />
    </VisualElement>
    <VisualElement class="unity-builder-code-row">
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-type" text="TextField" />
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-class-name" text=".yellow" />
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-pseudo-state" text=":hover" />
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-type" text=" Label" />
        <Label class="unity-builder-code-label--code unity-builder-code-label--element-name" text="#foo" />
    </VisualElement>
    <VisualElement class="unity-builder-code-row unity-builder-code-row--padded">
        <Label class="unity-builder-code-label" text="Match on all " />
        <Label class="unity-builder-code-label unity-builder-code-label--element-type" text="Label" />
        <Label class="unity-builder-code-label" text="s named " />
        <Label class="unity-builder-code-label unity-builder-code-label--element-name" text="#foo" />
        <Label class="unity-builder-code-label" text=" inside " />
        <Label class="unity-builder-code-label unity-builder-code-label--element-type" text="TextField" />
        <Label class="unity-builder-code-label" text="s " />
        <Label class="unity-builder-code-label" text="that have the " />
        <Label class="unity-builder-code-label unity-builder-code-label--element-class-name" text=".yellow " />
        <Label class="unity-builder-code-label" text="style class and " />
        <Label class="unity-builder-code-label" text="have " />
        <Label class="unity-builder-code-label" text="the " />
        <Label class="unity-builder-code-label" text="mouse " />
        <Label class="unity-builder-code-label" text="currently " />
        <Label class="unity-builder-code-label unity-builder-code-label--element-pseudo-state" text=":hover" />
        <Label class="unity-builder-code-label" text="ing " />
        <Label class="unity-builder-code-label" text="over them." />
    </VisualElement>
</UXML>