.unity-builder-library__controls-category-foldout > .unity-foldout__header > .unity-toggle {
    border-color: rgb(117, 117, 117);
    background-color: rgb(78, 78, 78);
}

.unity-builder-library__header-button-strip {
    border-color:rgb(37, 37, 37);
}

.plain-view__item--selected {
    background-color: #385C83;
}

.plain-view__item--selected-no-focus {
    background-color: #4D4D4D;
}

.plain-view__item:hover {
    background-color: #303030 /*env(--theme-line-background-color-hover);*/
}

.plain-view__item--selected:hover {
    background-color: #385C83;
}

.unity-builder-library__tree-item-edit-button {
    background-image: resource("com.unity.ui.builder/Icons/Dark/CodePreview/GoToDashboard");
}

.unity-builder-library__tree-item-edit-button:hover {
    background-image: resource("com.unity.ui.builder/Icons/Selected/CodePreview/GoToDashboard");
}
