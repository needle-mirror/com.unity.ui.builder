/*****************************************************************************/
/* Global                                                                      */
/*****************************************************************************/

.unity-builder-hidden {
    display: none;
}

/* TODO: Remove when this is fixed in trunk. */
.unity-base-slider__dragger {
    border-radius: 6px;
}

/*****************************************************************************/
/* Pane                                                                      */
/*****************************************************************************/

.unity-builder__pane {
    overflow: hidden;
    flex-shrink: 0;
}

.unity-builder-pane__header {
    padding: 2px;
    border-top-width: 1px;
    flex-direction: row;
}

.unity-builder-pane__sub-title-prefix {
    opacity: 0.5;
    padding: 0;
}

.unity-builder-pane__sub-title {
    opacity: 0.5;
}

.unity-builder-pane__toolbar {
    flex-grow: 1;
    align-items: flex-end;
}

.unity-builder-pane__splitter-obstructed .unity-builder-pane__header {
    border-top-width: 2px;
}

.unity-builder-pane__content-container {
    flex-grow: 1;
    overflow: hidden;
}

/*****************************************************************************/
/* Split View                                                                */
/*****************************************************************************/

.unity-builder__split-view {
    flex-grow: 1;
}

.unity-builder__split-view--vertical {
    flex-direction: column;
}

.unity-builder__split-view--horizontal {
    flex-direction: row;
}

/*****************************************************************************/
/* Explorer                                                                  */
/*****************************************************************************/

.unity-builder-explorer {
    flex-grow: 1;
}

.unity-builder-explorer--hidden Label {
    opacity: 0.5;
}

.unity-builder-explorer-tree-item-template-path {
    opacity: 0.5;
}

.unity-builder-explorer__header {
    border-top-width: 1px;
    border-bottom-width: 1px;
}

.unity-builder-explorer-tree-item-label-cont {
    flex-direction: row;
}

.unity-builder-explorer-tree-item-label {
    margin-top: 3px;
    padding-top: 0;
    padding-left: 1px;
    padding-right: 1px;
    margin-right: 4px;
    margin-left: 1px;
}

.unity-builder-explorer__reorder-zone {
    position: absolute;
    left: 0px;
    right: 0px;
    height: 5px;
}

.unity-builder-explorer__reorder-zone-above {
    top: 0px;
}

.unity-builder-explorer__reorder-zone-below {
    bottom: 0px;
}

.unity-builder-explorer__reorder-hover-bar {
    position: absolute;
    left: 0px;
    right: 0px;
    height: 2px;
}

.unity-builder-explorer__reorder-hover-bar-above {
    top: -1px;
}

.unity-builder-explorer__reorder-hover-bar-below {
    bottom: -1px;
}

.unity-builder-explorer__content-container {
    flex-direction: row;
    flex-grow: 1;
}

.unity-builder__toolbar-setting-menu {
    border-width: 0px;
    -unity-slice-top: 0;
    -unity-slice-right: 0;
}

.unity-builder__toolbar-setting-menu > .unity-toolbar-menu__arrow {
    background-image: none;
}

.unity-builder-explorer__rename-textfield__pre-2019-3 {
    padding-top: 2px;
    padding-bottom: 0;
    margin-top: 0;
    margin-bottom: 1px;
}

.unity-builder-explorer__rename-textfield {
    padding-top: 2px;
    padding-bottom: 0;
    margin-top: 0;
    margin-bottom: 2px;
}

.unity-builder-explorer__rename-textfield > .unity-text-input {
    padding-bottom: 3px;
}

.unity-builder-explorer__new-selector-item {
    flex-direction: row;
}

.unity-builder-explorer__new-selector-field-container {
    flex-grow: 1;
}

.unity-builder-explorer__new-selector-field {
    padding-top: 2px;
    padding-bottom: 0;
    margin-top: 0;
    margin-bottom: 1px;
}

.unity-builder-stylesheets__toolbar-add-pseudo-state-menu {
    padding-top: 2px;
}

.unity-builder-stylesheets__toolbar-add-new-selector {
    padding-top: 3px;
    padding-bottom: 0;
    margin-top: 0;
    margin-bottom: 1px;
}

.unity-builder-stylesheets-pane .unity-builder-explorer-tree-item-label {
    padding-left: 0;
    padding-right: 0;
    margin-right: 0;
    margin-left: 0;
}

/*****************************************************************************/
/* Viewport                                                                  */
/*****************************************************************************/

.unity-builder-viewport {
    flex-grow: 1;
    flex-shrink: 0;
}

.unity-builder-viewport__viewport-wrapper {
    flex-grow: 1;
    flex-shrink: 0;
    border-width: 0px;
    overflow: hidden;
    border-width: 4px;
    border-color: initial;
}

.unity-builder-viewport__viewport {
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    padding: 0px;
    flex-direction: column;
    align-content: center;
    align-items: flex-start;
}

.unity-builder-viewport__viewport.unity-builder-viewport--preview {
    overflow: hidden;
}


.unity-builder-viewport__viewport-surface {
    position: absolute;
}

.unity-builder-viewport__canvas {
    flex-shrink: 0;
}

.unity-builder-viewport__canvas-layer {
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
}

.unity-builder-viewport__shared-styles-and-document {
    border-width: 4px;
    margin: -4px;
}

.unity-builder-viewport__shared-styles {
    display: none;
}

.unity-builder-viewport__pick-overlay.unity-builder-viewport--preview {
    display: none;
}

.unity-builder-viewport__fit-canvas-button {
    position: absolute;
    right: 5px;
    bottom: 5px;
    display: none; /* Starts hidden. */
}

/*****************************************************************************/
/* Builder Canvas                                                            */
/*****************************************************************************/

.unity-builder-canvas__container {
    flex-grow: 1;
}

.unity-builder-canvas__handle {
    position: absolute;
}

.unity-builder-canvas__corner {
    width: 24px;
    height: 24px;
}

.unity-builder-canvas__side--horizontal {
    height: 18px;
    cursor: resize-vertical;
    flex-direction: row;
}

.unity-builder-canvas__side--vertical {
    width: 18px;
    cursor: resize-horizontal;
    flex-direction: column;
}

.unity-builder-canvas__side--top {
    top: -22px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    left: 0px;
    right: 0px;
}

.unity-builder-canvas__side--left {
    top: 0px;
    left: -22px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    bottom: 0px;
}

.unity-builder-canvas__side--bottom {
    left: 0px;
    right: 0px;
    bottom: -22px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
}

.unity-builder-canvas__side--right {
    top: 0px;
    right: -22px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    bottom: 0px;
}

.unity-builder-canvas__corner--top-left {
    top: -28px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    left: -28px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    cursor: resize-up-left;
}

.unity-builder-canvas__corner--top-right {
    top: -28px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    right: -28px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    cursor: resize-up-right;
}

.unity-builder-canvas__corner--bottom-left {
    bottom: -28px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    left: -28px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    cursor: resize-up-right;
}

.unity-builder-canvas__corner--bottom-right {
    bottom: -28px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    right: -28px; /* -4 from the .unity-builder-viewport__shared-styles-and-document margin: -4px; */
    cursor: resize-up-left;
}

.unity-builder-canvas__side__inner {
    position: absolute;
}

.unity-builder-canvas__side--top .unity-builder-canvas__side__inner {
    height: 4px;
    left: 0px;
    right: 0px;
    bottom: 0px;
}

.unity-builder-canvas__side--left .unity-builder-canvas__side__inner {
    width: 4px;
    top: 0px;
    bottom: 0px;
    right: 0px;
}

.unity-builder-canvas__side--bottom .unity-builder-canvas__side__inner {
    height: 4px;
    left: 0px;
    right: 0px;
    top: 0px;
}

.unity-builder-canvas__side--right .unity-builder-canvas__side__inner {
    width: 4px;
    top: 0px;
    bottom: 0px;
    left: 0px;
}

.unity-builder-canvas__corner__inner {
    position: absolute;
    width: 12px;
    height: 12px;
}

.unity-builder-canvas__corner--top-left .unity-builder-canvas__corner__inner {
    right: 0px;
    bottom: 0px;
}

.unity-builder-canvas__corner--top-right .unity-builder-canvas__corner__inner {
    left: 0px;
    bottom: 0px;
}

.unity-builder-canvas__corner--bottom-left .unity-builder-canvas__corner__inner {
    right: 0px;
    top: 0px;
}

.unity-builder-canvas__corner--bottom-right .unity-builder-canvas__corner__inner {
    left: 0px;
    top: 0px;
}

.unity-builder-canvas__cursor-setter {
    position: absolute;
    top: 0px;
    left: 0px;
    bottom: 0px;
    right: 0px;
}

.unity-builder-canvas__side--horizontal .unity-builder-canvas__cursor-setter {
    cursor: resize-vertical;
}

.unity-builder-canvas__side--vertical .unity-builder-canvas__cursor-setter {
    cursor: resize-horizontal;
}

.unity-builder-canvas__corner--top-left .unity-builder-canvas__cursor-setter {
    cursor: resize-up-left;
}

.unity-builder-canvas__corner--top-right .unity-builder-canvas__cursor-setter {
    cursor: resize-up-right;
}

.unity-builder-canvas__corner--bottom-left .unity-builder-canvas__cursor-setter {
    cursor: resize-up-right;
}

.unity-builder-canvas__corner--bottom-right .unity-builder-canvas__cursor-setter {
    cursor: resize-up-left;
}

.unity-builder-canvas__drag-hover-cover-layer {
    display: none;
    position: absolute;
    top: -20px;
    left: -20px;
    bottom: -20px;
    right: -20px;
}

.unity-builder-viewport__canvas-layer.unity-builder-canvas__container--dark,
.unity-builder-tooltip-preview.unity-builder-canvas__container--dark {
    color: #D9D9D9;
    background-color: rgb(56, 56, 56);
}

.unity-builder-viewport__canvas-layer.unity-builder-canvas__container--light,
.unity-builder-tooltip-preview.unity-builder-canvas__container--light {
    color: #000;
    background-color: rgb(182, 182, 182);
}

.unity-builder-canvas__container--runtime {
    color: #000;
    background-color: rgb(40, 61, 75);
}

.unity-builder-canvas__special-visual-element-initial-size {
    min-width: 100px;
    min-height: 100px;
    border-width: 1px;
    flex-grow: 1;
}

/*****************************************************************************/
/* Snippet Content                                                           */
/*****************************************************************************/

.unity-builder-code-preview {
    flex-grow: 1;
}

.unity-builder-code__scroll-view {
    flex-grow: 1;
    flex-shrink: 0;
}

.unity-builder-code__title {
    -unity-font-style: bold;
    margin-top: 8px;
}

.unity-builder-code__code {
    height: auto;
}

.unity-builder-code__code-text {
    border-width: 1px;
    padding: 4px;
}

.unity-builder-code__code-line-numbers {
    min-width: 30px;
    -unity-text-align: upper-right;
    -unity-font: resource("Consola");
    font-size: 13px;
    padding: 6px;
}

.unity-builder-code__code_outer_container {
    flex-direction: row;
}

.unity-builder-code__code_container {
    flex-direction: row;
    overflow: hidden;
}

.unity-base-text-field .unity-builder-code__input {
    -unity-font: resource("Consola");
    font-size: 13px;
    background-image: none;
    border-color: initial;
}

.unity-base-text-field .unity-builder-code__input:active,
.unity-base-text-field .unity-builder-code__input:hover,
.unity-base-text-field .unity-builder-code__input:focus {
    background-image: none;
    border-color: initial;
}

.unity-builder-code__open-source-file-button {
    background-color: initial;
    border-radius: 0;
    border-width: 0;
    width: 15px;
    height: 15px;
}

/*****************************************************************************/
/* Library                                                                   */
/*****************************************************************************/

.unity-builder-library {
    flex-grow: 1;
}

.unity-builder-library__tree-view {
    flex-grow: 1;
}

.unity-builder-library__tree-item {
    flex-direction: row;
    flex-grow: 1;
    flex-shrink: 0;
    flex-basis: 0;
}

.unity-builder-library__tree-item-label {
    flex-grow: 1;
    -unity-text-align: middle-left;
}

/*****************************************************************************/
/* Builder Class Pill                                                        */
/*****************************************************************************/

.unity-builder-class-pill {
    flex-direction: row;
    margin: 2px;
    flex-shrink: 0;
    border-radius: 4px;
    padding-left: 4px;
    padding-right: 4px;
    overflow: hidden;
}

.unity-builder-class-pill Label {
    max-width: 200px;
    overflow: hidden;
    -unity-text-align: middle-left;
}

.unity-builder-class-pill__delete-button {
    height: 16px;
    margin-right: 0px;
    padding-bottom: 2px;
    margin-bottom: 2px;
    margin-top: 0px;
    -unity-text-align: middle-center;
}

.unity-builder-class-pill__delete-button,
.unity-builder-class-pill__delete-button:active,
.unity-builder-class-pill__delete-button:hover,
.unity-builder-class-pill__delete-button:focus {
    background-image: none;
    background-color: initial;
    border-color: initial;
}

.unity-debugger-tree-item-pill.unity-builder-class-pill {
    margin-top: 1px;
    margin-bottom: 1px;
    padding-top: 0;
    padding-bottom: 0;
}

.unity-builder-class-pill--draggable .unity-builder-class-pill__delete-button,
.unity-debugger-tree-item-pill.unity-builder-class-pill .unity-builder-class-pill__delete-button {
    display: none;
}

/*****************************************************************************/
/* Dragger                                                                   */
/*****************************************************************************/

.unity-builder-dragger-preview {
    display: none;
    position: absolute;
}

.unity-builder-dragger-preview.unity-builder-dragger-preview--dragged {
    display: flex;
}

/*****************************************************************************/
/* Toolbar                                                                   */
/*****************************************************************************/

.unity-builder-toolbar * {
    flex-shrink: 0;
}

.unity-builder__toolbar-flex-space {
    flex-grow: 1;
}

.unity-builder__theme-toolbar-menu {
    width: 120px;
    padding-top: 2px;
}

.unity-builder__zoom-toolbar-menu {
    min-width: 60px;
}

/*****************************************************************************/
/* Save Dialog                                                               */
/*****************************************************************************/

.unity-builder__save-dialog-gutter {
    flex-direction: row;
    align-self: flex-end;
}

.unity-builder__save-dialog-validation-container {
    height: 40px;
}

.unity-builder__save-dialog-path-field-container {
    flex-grow: 1;
}

.unity-builder__save-dialog-container .unity-text-field {
    flex-grow: 1;
}

.unity-builder__save-dialog-container .unity-button {
    padding-left: 3px;
}

.unity-builder__save-dialog-container .unity-text-field .unity-base-field__label {
    width: 50px;
    min-width: 50px;
}

.unity-builder__save-dialog-path-field {
    flex-direction: row;
    margin-bottom: 10px;
}

/*****************************************************************************/
/* Document Preview Window                                                   */
/*****************************************************************************/

.unity-builder-preview__uxml-asset-field {
    margin: 2px;
    flex-direction: column;
    height: 34px;
}

.unity-builder-preview__container {
    border-top-width: 2px;
    margin-top: 2px;
}

/*****************************************************************************/
/* Builder Tracker                                                           */
/*****************************************************************************/

.unity-builder-tracker {
    position: absolute;
    display: none;
}

.unity-builder-tracker--active {
    display: flex;
}

/*****************************************************************************/
/* Builder Parent Tracker                                                    */
/*****************************************************************************/

.unity-builder-parent-tracker {
    border-width: 2px;
}

/*****************************************************************************/
/* Builder Transformer                                                       */
/*****************************************************************************/

.unity-builder-transformer {
    position: absolute;
}

.unity-builder-transformer__drag-hover-cover-layer {
    display: none;
    position: absolute;
    top: -20px;
    left: -20px;
    bottom: -20px;
    right: -20px;
}

/*****************************************************************************/
/* Builder Mover                                                             */
/*****************************************************************************/

.unity-builder-mover {
    position: absolute;
    cursor: move-arrow;
}

/*****************************************************************************/
/* Builder Resizer                                                           */
/*****************************************************************************/

.unity-builder-resizer {
    position: absolute;
}

.unity-builder-resizer__corner {
    width: 18px;
    height: 18px;
}

.unity-builder-resizer__side--horizontal {
    height: 12px;
    cursor: resize-vertical;
    flex-direction: row;
}

.unity-builder-resizer__side--vertical {
    width: 12px;
    cursor: resize-horizontal;
    flex-direction: column;
}

.unity-builder-resizer__side--top {
    top: -12px;
    left: 0px;
    right: 0px;
}

.unity-builder-resizer__side--left {
    top: 0px;
    left: -12px;
    bottom: 0px;
}

.unity-builder-resizer__side--bottom {
    left: 0px;
    right: 0px;
    bottom: -12px;
}

.unity-builder-resizer__side--right {
    top: 0px;
    right: -12px;
    bottom: 0px;
}

.unity-builder-resizer__corner--top-left {
    top: -18px;
    left: -18px;
    cursor: resize-up-left;
}

.unity-builder-resizer__corner--top-right {
    top: -18px;
    right: -18px;
    cursor: resize-up-right;
}

.unity-builder-resizer__corner--bottom-left {
    bottom: -18px;
    left: -18px;
    cursor: resize-up-right;
}

.unity-builder-resizer__corner--bottom-right {
    bottom: -18px;
    right: -18px;
    cursor: resize-up-left;
}

.unity-builder-resizer__side__inner {
    position: absolute;
}

.unity-builder-resizer__side--top .unity-builder-resizer__side__inner {
    height: 2px;
    left: 0px;
    right: 0px;
    bottom: 0px;
}

.unity-builder-resizer__side--left .unity-builder-resizer__side__inner {
    width: 2px;
    top: 0px;
    bottom: 0px;
    right: 0px;
}

.unity-builder-resizer__side--bottom .unity-builder-resizer__side__inner {
    height: 2px;
    left: 0px;
    right: 0px;
    top: 0px;
}

.unity-builder-resizer__side--right .unity-builder-resizer__side__inner {
    width: 2px;
    top: 0px;
    bottom: 0px;
    left: 0px;
}

.unity-builder-resizer__corner__inner {
    position: absolute;
    width: 6px;
    height: 6px;
}

.unity-builder-resizer__corner--top-left .unity-builder-resizer__corner__inner {
    right: 0px;
    bottom: 0px;
}

.unity-builder-resizer__corner--top-right .unity-builder-resizer__corner__inner {
    left: 0px;
    bottom: 0px;
}

.unity-builder-resizer__corner--bottom-left .unity-builder-resizer__corner__inner {
    right: 0px;
    top: 0px;
}

.unity-builder-resizer__corner--bottom-right .unity-builder-resizer__corner__inner {
    left: 0px;
    top: 0px;
}

.unity-builder-resizer__cursor-setter {
    position: absolute;
    top: 0px;
    left: 0px;
    bottom: 0px;
    right: 0px;
}

.unity-builder-resizer__side--horizontal .unity-builder-resizer__cursor-setter {
    cursor: resize-vertical;
}

.unity-builder-resizer__side--vertical .unity-builder-resizer__cursor-setter {
    cursor: resize-horizontal;
}

.unity-builder-resizer__corner--top-left .unity-builder-resizer__cursor-setter {
    cursor: resize-up-left;
}

.unity-builder-resizer__corner--top-right .unity-builder-resizer__cursor-setter {
    cursor: resize-up-right;
}

.unity-builder-resizer__corner--bottom-left .unity-builder-resizer__cursor-setter {
    cursor: resize-up-right;
}

.unity-builder-resizer__corner--bottom-right .unity-builder-resizer__cursor-setter {
    cursor: resize-up-left;
}

/*****************************************************************************/
/* Builder Anchorer                                                          */
/*****************************************************************************/

.unity-builder-anchorer {
    position: absolute;
    align-content: center;
    align-items: center;
    justify-content: center;
}

.unity-builder-anchorer__hover-zone {
    width: 12px;
    height: 12px;
    align-content: center;
    align-items: center;
    justify-content: center;
}

.unity-builder-anchorer__inner {
    width: 5px;
    height: 5px;
    border-width: 1px;
}

.unity-builder-anchorer--top {
    top: -24px;
    left: 0px;
    right: 0px;
}

.unity-builder-anchorer--left {
    top: 0px;
    left: -24px;
    bottom: 0px;
}

.unity-builder-anchorer--bottom {
    left: 0px;
    right: 0px;
    bottom: -24px;
}

.unity-builder-anchorer--right {
    top: 0px;
    right: -24px;
    bottom: 0px;
}

/*****************************************************************************/
/* Builder Dragger                                                           */
/*****************************************************************************/

.unity-builder-dragger__overlay {
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
}

.unity-builder-dragger__container {
    position: absolute;
}

.unity-builder-dragger__empty-vs {
    border-width: 1px;
    width: 100px;
    height: 100px;
}

.unity-builder-dragger__drag-preview {
    opacity: 0.5;
}

.unity-builder-dragger--being-dragged.unity-button {
    width: 100px;
}

.unity-builder-dragger--being-dragged.unity-base-field {
    width: 150px;
}

.unity-builder-dragger--being-dragged > .unity-base-field__label {
    min-width: 0px;
    width: 80px;
}

/*****************************************************************************/
/* Tooltip Preview                                                           */
/*****************************************************************************/

.unity-builder-tooltip-preview {
    position: absolute;
    margin-top: 1px;
}

.unity-builder-tooltip-preview__enabler {
    display: none;
    border-width: 1px;
}

.unity-builder-tooltip-preview__container {
    overflow: hidden;
    min-width: 250px;
    max-width: 400px;
    min-height: 20px;
    max-height: 250px;
    padding: 8px;
}

.unity-builder-tooltip-preview * {
    flex-shrink: 0;
}

.unity-builder-tooltip-preview .unity-base-field__label {
    min-width: 0px;
    width: 100px;
}

/*****************************************************************************/
/* Code                                                                      */
/*****************************************************************************/

.unity-builder-code-row {
    flex-direction: row;
    flex-wrap: wrap;
}

.unity-builder-code-row--padded {
    margin-top: 10px;
}

.unity-builder-inspector__selector-example {
    padding: 10px;
}

.unity-builder-code-label {
    padding: 0;
    white-space: normal;
}

.unity-builder-code-label--code {
    margin-top: 4px;
    padding: 0;
    -unity-font: resource("Consola");
    font-size: 13px;
}
